    "calories = []\n",
    "with open('input.txt', 'r') as f:\n",
    "    calory = 0\n",
    "    for line in f:\n",
    "        if line != '\\n':\n",
    "            calory += int(line)\n",
    "        else:\n",
    "            calories.append(calory)\n",
    "            calory = 0"
